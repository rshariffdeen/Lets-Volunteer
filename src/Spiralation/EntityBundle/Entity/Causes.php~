<?php

namespace Spiralation\EntityBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Causes
 */
class Causes
{
    /**
     * @var string
     */
    private $causeName;

    /**
     * @var integer
     */
    private $causeId;


    /**
     * Set causeName
     *
     * @param string $causeName
     * @return Causes
     */
    public function setCauseName($causeName)
    {
        $this->causeName = $causeName;
    
        return $this;
    }

    /**
     * Get causeName
     *
     * @return string 
     */
    public function getCauseName()
    {
        return $this->causeName;
    }

    /**
     * Get causeId
     *
     * @return integer 
     */
    public function getCauseId()
    {
        return $this->causeId;
    }
}
