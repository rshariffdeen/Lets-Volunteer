                {% if message is defined %}
                <div class="alert alert-info">
                    {{message}}
                </div>
                {% endif %}
                <form class="form-horizontal" action="index.html" method="post">
                    <fieldset>
                        <div class="input-prepend" title="Username" data-rel="tooltip">
                            <span class="add-on"><i class="icon-user"></i></span><input autofocus class="input-large span10" name="username" id="username" type="text" value="admin" />
                        </div>
                        <div class="clearfix"></div>

                        <div class="input-prepend" title="Password" data-rel="tooltip">
                            <span class="add-on"><i class="icon-lock"></i></span><input class="input-large span10" name="password" id="password" type="password" value="admin123456" />
                        </div>
                       
                         <p class="center span5">
                             <button type="submit" class="btn btn-success">Login</button> <br><br>
                             Not a user? Register Now!
                        </p>
                        

                       
                    </fieldset>
                </form>
            </div><!--/span--><p align="center">
             <button type="submit" class="btn btn-primary" onclick="Login">Login with Facebook</button>&nbsp;&nbsp;&nbsp;
                <button class="g-signin btn btn-warning"
                        data-scope="https://www.googleapis.com/auth/plus.login"
                        data-clientid="982349219563-7c0o30oj67mu6l206td9f7i40afdlcel.apps.googleusercontent.com"
                        data-callback="onSignInCallback"
                        data-theme="dark"
                        data-cookiepolicy="single_host_origin"
                        data-requestvisibleactions="http://schemas.google.com/AddActivity"
                        data-width="wide">
                    Login with Google+</button>
            </p>
            <div id="fb-root"></div>
            <fb:login-button autologoutlink='true'  perms='email,user_birthday,status_update,publish_stream'></fb:login-button>
        </div><!--/row-->
       
