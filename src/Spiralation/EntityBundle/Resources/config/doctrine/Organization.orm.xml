<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Spiralation\EntityBundle\Entity\Organization" table="organization">
    <id name="organizationId" type="integer" column="organization_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="false"/>
    <field name="city" type="string" column="city" length="30" nullable="false"/>
    <field name="details" type="text" column="details" nullable="false"/>
    <field name="type" type="string" column="type" length="20" nullable="false"/>
    <field name="rating" type="float" column="rating" precision="10" scale="0" nullable="true"/>
    <field name="followers" type="string" column="followers" length="1000" nullable="false"/>
    <field name="projects" type="string" column="projects" length="1000" nullable="false"/>
    <field name="email" type="string" column="email" length="40" nullable="false"/>
    <field name="phone" type="integer" column="phone" nullable="false"/>
    <field name="password" type="string" column="password" length="40" nullable="false"/>
  </entity>
</doctrine-mapping>
